<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:property-placeholder location="classpath:Message-en.properties" />

	<bean id="EnglishProperties" class="com.ac.pojo.LoginPagePojo">
		<property name="title" value="${title}" />
		<property name="shopName" value="${shopName}" />
		<property name="admin" value="${admin}" />
		<property name="loginby" value="${loginby}" />
		<property name="submitbutton" value="${submitbutton}" />
		<property name="user" value="${user}" />
		<property name="pswd" value="${pswd}" />
		<property name="username" value="${username}" />
		<property name="clearbutton" value="${clearbutton}" />
		<property name="welcome" value="${welcome}" />
		<property name="customer" value="${customer}" />
		<property name="addUser" value="${addUser}" />
		<property name="getUserDetails" value="${getUserDetails}" />
		<property name="deleteUser" value="${deleteUser}" />

	</bean>
</beans>